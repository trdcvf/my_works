function getContents(parent,content)
{
	var childs = parent.getElementsByTagName("*");
	var contents = []; 
	for (var i = 0; i < childs.length; i++) 
	{
		if(childs[i].className == content)
			contents.push(childs[i]); 
	};
	return contents;
